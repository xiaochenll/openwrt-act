#!name=搜索命令
#!desc=最后更新时间 2022-03-23
[URL Rewrite]
#Q-Search

# - Safari 内输入 命令 + 空格 + 关键词 快速指定搜索引擎搜索
#注：先进入设置更改 Safari 默认搜索为 google
#可自行修改指令或者添加搜索引擎

# ＞搜索
# gm   (Google图片)
^https:\/\/google.cn\/search\?q=gm\+([^&]+).+ https://www.google.com/search?&tbm=isch&q=$1 302
# gh   (GitHub)
^https:\/\/google.cn\/search\?q=gh\+([^&]+).+ https://github.com/search?q=$1 302
# sof  (Stack Overflow)
^https:\/\/google.cn\/search\?q=sof\+([^&]+).+ https://stackoverflow.com/search?q=$1 302
# se  (StackExchange)
^https:\/\/google.cn\/search\?q=se\+([^&]+).+ https://stackexchange.com/search?q=$1 302
# wa  (WolframAlpha)
^https:\/\/google.cn\/search\?q=wa\+([^&]+).+ https://www.wolframalpha.com/input/?i=$1 302
# wiki  (维基百科)
^https:\/\/google.cn\/search\?q=wiki(\+|%20)([^&]+).+ http://wikipedia.org/wiki/$2 302
# wk  (维基中文)
^https:\/\/google.cn\/search\?q=wk(\+|%20)([^&]+).+ https://zh.wikipedia.org/wiki/$2 302
# mg  (Magi)
^https:\/\/google.cn\/search\?q=mg(\+|%20)([^&]+).+ https://magi.com/search?q=$2 302
# tf  (Google 搜索 TestFlight)
^https:\/\/google.cn\/search\?q=tf(\+|%20)([^&]+).+ https://www.google.cn/search?as_q=$2&as_sitesearch=testflight.apple.com 302

# > 翻译
# yd   (有道)
^https:\/\/google.cn\/search\?q=yd\+([^&]+).+ http://dict.youdao.com/search?q=$1 302
# trc  (Google 译至中)
^https:\/\/google.cn\/search\?q=trc\+([^&]+).+ https://translate.google.cn/#view=home&op=translate&sl=auto&tl=zh-CN&text=$1 302
# tre  (Google 译至英)
^https:\/\/google.cn\/search\?q=tre\+([^&]+).+ https://translate.google.cn/#view=home&op=translate&sl=auto&tl=en&text=$1 302
# trj  (Google 译至日)
^https:\/\/google.cn\/search\?q=trj\+([^&]+).+ https://translate.google.cn/#view=home&op=translate&sl=auto&tl=ja&text=$1 302

# ＞社区
# db：豆瓣
^https:\/\/google.cn\/search\?q=db\+([^&]+).+ https://m.douban.com/search/?query=$1 302
# zh: 知乎
^https:\/\/google.cn\/search\?q=zh\+([^&]+).+ https://www.zhihu.com/search?type=content&q=$1 302
# wb: 微博
^https:\/\/google.cn\/search\?q=wb\+([^&]+).+ https://s.weibo.com/weibo/$1 302
# wx: 微信
^https:\/\/google.cn\/search\?q=wx\+([^&]+).+ https://weixin.sogou.com/weixinwap?query=$1 302
# rd  (Reddit)
^https:\/\/google.cn\/search\?q=rd\+([^&]+).+ https://www.reddit.com/search?q=$1 302
# tw (Twitter)
^https:\/\/google.cn\/search\?q=tw\+([^&]+).+ https://twitter.com/search?q=$1 302
# ssp: 少数派
^https:\/\/google.cn\/search\?q=ssp\+([^&]+).+ https://sspai.com/search/post/$1 302
# csdn: CSDN
^https:\/\/google.cn\/search\?q=csdn\+([^&]+).+ https://so.csdn.net/so/search/s.do?q=$1&t=&u= 302

# > 购物
# zdm: 什么值得买
^https:\/\/google.cn\/search\?q=zdm\+([^&]+).+ https://search.m.smzdm.com/?v=b&s=$1 302
# amz: 亚马逊
^https:\/\/google.cn\/search\?q=amz\+([^&]+).+ https://www.amazon.cn/gp/aw/s/?k=$1 302
# jd : 京东
^https:\/\/google.cn\/search\?q=jd\+([^&]+).+ https://so.m.jd.com/ware/search.action?keyword=$1 302
# tb: 淘宝
^https:\/\/google.cn\/search\?q=tb\+([^&]+).+ https://s.m.taobao.com/h5?q=$1 302
# tm: 天猫
^https:\/\/google.cn\/search\?q=tm\+([^&]+).+ https://s.m.tmall.com/m/search.htm?q=$1 302

# ＞视频资源
# ac (Acfun)
^https:\/\/google.cn\/search\?q=ac\+([^&]+).+ https://www.acfun.cn/search?keyword=$1 302
# bli (哔哩哔哩) 
^https:\/\/google.cn\/search\?q=bli\+([^&]+).+ https://m.bilibili.com/search?keyword=$1 302
# ytb  (YouTube)
^https:\/\/google.cn\/search\?q=ytb\+([^&]+).+ https://www.youtube.com/results?search_query=$1 302
# ph  (PornHub) 
^https:\/\/google.cn\/search\?q=ph\+([^&]+).+ https://cn.pornhub.com/video/search?search=$1 302

# ＞网盘
# gd  (Google Drive)
^https:\/\/google.cn\/search\?q=gd\+([^&]+).+ https://www.google.cn/search?q=%22Google+Drive%22+$1 302
# tgd  (t.me/gdurl 搜索 Google Drive 资源)
^https:\/\/google.cn\/search\?q=tgd\+([^&]+).+ https://t.me/s/gdurl?q=$1 302
# zgd  (https://zhao.pp.ua 搜索 Google Drive 资源)
^https:\/\/google.cn\/search\?q=zgd(\+|%20)([^&]+).+ https://zhao.pp.ua/?q=$2 302

# ＞默认搜索
# bi: 必应
^https:\/\/google.cn\/search\?q=bi\+([^&]+).+ https://cn.bing.com/search?q=$1 302
# bd: 百度
^https:\/\/google.cn\/search\?q=bd\+([^&]+).+ https://www.baidu.com/s?wd=$1 302
# ddg: DuckDuckGo 
^https:\/\/google.cn\/search\?q=ddg\+([^&]+).+ https://duckduckgo.com/?ia=about&q=$1 302
# 无指令: 谷歌
^https:\/\/google.cn\/search\?q=([^&]+).+ https://www.google.com/search?q=$1 302

[MITM]
hostname = google.cn
